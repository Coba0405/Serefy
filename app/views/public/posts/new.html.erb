public/post/new
<h1>新規投稿</h1>
<%= form_with model: @post, url: public_posts_path do |f| %>
  <%= f.label :投稿画像%>
  <%= f.file_field :image, accept: "image/*" %>
  
  <%= f.label :Title%>
  <%= f.text_field :title %>
  
  <%= f.label :Body %>
  <%= f.text_area :body %>

  <%= f.submit "投稿" %>
<% end %>
